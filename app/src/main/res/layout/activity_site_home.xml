<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:context=".ui.home.siteEngineer.SiteHomeActivity">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar_se_home"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="?colorPrimary" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="0.6"
            android:orientation="vertical"
            android:stateListAnimator="@null">

            <com.google.android.material.tabs.TabLayout
                android:id="@+id/TabLayout_home_se"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#1db995"></com.google.android.material.tabs.TabLayout>

            <androidx.viewpager.widget.ViewPager
                android:id="@+id/viewPager_home_se"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/TabLayout_home_se"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="0dp"
                tools:layout_editor_absoluteX="8dp" />
        </RelativeLayout>
    </LinearLayout>
</layout>